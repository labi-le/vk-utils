{
  "name": "labile/vk-utils",
  "description": "PHP Utils for vk.com API",
  "minimum-stability": "stable",
  "keywords": [
    "php",
    "vk-utils",
    "vk-utilites",
    "api",
    "vk",
    "vk.com",
    "client",
    "vkontakte"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "labile",
      "email": "1a6i1e@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "spatie/fork": "^1.1",
    "ext-fileinfo": "*"
  },

  "autoload": {
    "psr-4": {
      "Astaroth\\VkUtils\\": "src"
    },
    "files": ["src/functions.php"]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
